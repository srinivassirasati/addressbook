pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "mymaven"
    }

    stages {
        stage('compile') {
            steps {
                echo "Compiling the code"
                
            }

           
        }
        stage('UnitTest') {
            steps {
                echo "Test the code"
                
            }

           
        }
        stage('package') {
            steps {
                echo "package the code"
                
            }

           
        }
    }
}
